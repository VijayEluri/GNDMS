 <ns10:ServiceProperties xmlns:ns10="gme://gov.nih.nci.cagrid.introduce/1/Property">
  <ns10:Property description="Properties file containing type mappings. For each key ending in &quot;.orfType&quot; its value is mapped onto the value of the property stored unter this keys names appended with &quot;.port&quot;" isFromETC="true" key="orfMappingFile" value="orfMapping.properties"/>
  <ns10:Property description="Minimum interval between actual executions of updateMapping(). Requests received in-between simply do nothing." isFromETC="false" key="minimumUpdateInterval" value="10000"/>
 </ns10:ServiceProperties>
